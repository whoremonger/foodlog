
<div id="<%= dom_id entry %>">
<div class="card">
  <header class="card-header">
    <p class="card-header-title"><%= entry.meal_type %></p>
  </header>
  <div class="card-content">
    <div class="content">
      <p><span class="has-text-weight-bold">Calories: <%= entry.calories %></span></p>
      <p><span class="has-text-weight-bold">Proteins: <%= entry.proteins %></span></p>
      <p><span class="has-text-weight-bold">Carbohydrates: <%= entry.carbohydrates %></span></p>
      <p><span class="has-text-weight-bold">Fats: <%= entry.fats %></span></p>
      <p class="italic">Logged: <%= time_ago_in_words(entry.created_at) %></p>
    </div>
  </div>
  <div class="card-footer">
  
  </div>
</div>
</div>

